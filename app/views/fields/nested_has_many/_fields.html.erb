<div class="nested-fields nhsuk-panel">
  <% field.nested_fields_for_builder(f).each do |attribute| -%>
    <%= render_field attribute, f: f, parent_field: field, errors: defined?(errors) && errors %>
  <% end -%>

  <div class='nhsuk-form-group'>
    <%= link_to_remove_association I18n.t("administrate.fields.nested_has_many.remove", resource: field.associated_class_name.titleize), f, class: 'nhsuk-button nhsuk-button--secondary' %>
  </div>
</div>
